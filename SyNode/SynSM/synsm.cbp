				</Linker>
				<Environment>
					<Variable name="MOZJS_BUILD_FLDR" value="_OPT.OBJ" />
					<Variable name="MOZJS_SRC_DIR" value="../../../gecko-dev" />
				</Environment>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wunreachable-code" />
			<Add option="-Wall" />
			<Add option="-std=c++14" />
			<Add option="-fPIC" />
			<Add option="-m64" />
			<Add option="-Wc++11-compat" />
