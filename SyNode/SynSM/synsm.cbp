				</Compiler>
				<Environment>
					<Variable name="MOZJS_BUILD_FLDR" value="_DBG.OBJ" />
					<Variable name="MOZJS_SRC_DIR" value="../../../gecko-dev" />
				</Environment>
			</Target>
			<Target title="Release">
